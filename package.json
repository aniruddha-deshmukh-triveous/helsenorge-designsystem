{
  "name": "hn-designsystem",
  "version": "1.0.0",
  "private": true,
  "description": "NPM pakke og dokumentasjon for Helsenorges designsystem",
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/nhnfelles/Helsenorge/_git/HN-Designsystem"
  },
  "homepage": "https://helsenorge.design",
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "bootstrap:ci": "lerna bootstrap --npm-ci-mode",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "test:update": "lerna run test:update",
    "version": "lerna version --no-push --allow-branch=*/*",
    "postversion": "lerna run build",
    "publish:beta": "cd npm/designsystem/dist && npm publish --tag=beta",
    "publish:latest": "lerna publish from-git --yes --allow-branch=master",
    "format": "lerna run format",
    "validate": "lerna run validate",
    "clean": "lerna run clean"
  },
  "dependencies": {
    "eslint-plugin-html": "^6.0.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.0.3",
    "lerna": "^3.22.1",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.0",
    "typescript": "~3.8.3"
  }
}
